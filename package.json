{
  "name": "advents-of-code",
  "version": "1.0.0",
  "description": "Just a dump of any advents of code I am about to go through",
  "main": "index.js",
  "scripts": {
    "aoc:run": "deno run --allow-read --allow-hrtime ./src/adventofcode/run.ts",
    "aoc:fetch": "deno run --allow-net --allow-write --allow-read ./src/adventofcode/fetch.ts",
    "format": "prettier --write ."
