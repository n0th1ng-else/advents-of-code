  "license": "MIT",
  "devDependencies": {
    "prettier": "^3.1.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format"
  }
}
