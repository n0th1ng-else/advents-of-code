{
  "name": "advents-of-code",
  "version": "1.0.0",
  "description": "Just a dump of any advents of code I am about to go through",
  "main": "index.js",
  "scripts": {
    "aoc:run": "deno run --allow-read --allow-hrtime ./src/adventofcode/scripts/run.ts",
    "aoc:fetch": "deno run --allow-net --allow-write --allow-read ./src/adventofcode/scripts/fetch.ts",
    "format": "prettier --write .",
    "typecheck": "deno check **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:n0th1ng-else/advents-of-code.git"
  },
  "author": "Sergey Nikitin <srg.post@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "prettier": "3.4.1",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.7.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run format"
  }
}
